<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	
	<application>
        <el-resolver>
        	org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>

    <navigation-rule> 
    
    <from-view-id>*</from-view-id> 
    
    <!-- Login -->

    <navigation-case> 
      <from-outcome>index</from-outcome> 
      <to-view-id>index.xhtml</to-view-id> 
    </navigation-case> 
    
     <!-- Panel -->
    
   <navigation-case> 
      <from-outcome>panel</from-outcome> 
      <to-view-id>inicio/panel.xhtml</to-view-id> 
    </navigation-case>

<!-- Usuario -->
   <navigation-case> 
      <from-outcome>usuario_registro</from-outcome> 
      <to-view-id>maestro/usuario_registro.xhtml</to-view-id> 
    </navigation-case>
    
       <navigation-case> 
      <from-outcome>usuario_listado</from-outcome> 
      <to-view-id>maestro/usuario_listado.xhtml</to-view-id> 
    </navigation-case>
   
<!-- Investigador -->
   <navigation-case> 
      <from-outcome>inv_registro</from-outcome> 
      <to-view-id>maestro/inv_registro.xhtml</to-view-id> 
    </navigation-case>
    
       <navigation-case> 
      <from-outcome>inv_listado</from-outcome> 
      <to-view-id>maestro/inv_listado.xhtml</to-view-id> 
    </navigation-case>
    
    <!-- Partida -->
   <navigation-case> 
      <from-outcome>partida_registro</from-outcome> 
      <to-view-id>maestro/partida_registro.xhtml</to-view-id> 
    </navigation-case>
    
       <navigation-case> 
      <from-outcome>partida_listado</from-outcome> 
      <to-view-id>maestro/partida_listado.xhtml</to-view-id> 
    </navigation-case>
    
        <!-- Proyecto -->
   <navigation-case> 
      <from-outcome>proy_registro</from-outcome> 
      <to-view-id>maestro/proy_registro.xhtml</to-view-id> 
    </navigation-case>
    
       <navigation-case> 
      <from-outcome>proy_listado</from-outcome> 
      <to-view-id>maestro/proy_listado.xhtml</to-view-id> 
    </navigation-case>
    
    <!-- Tramite -->
    <navigation-case> 
      <from-outcome>tramite_listado</from-outcome> 
      <to-view-id>procesos/tramite_listado.xhtml</to-view-id> 
    </navigation-case>
    
   <navigation-case> 
      <from-outcome>tramite_registro</from-outcome> 
      <to-view-id>procesos/tramite_registro.xhtml</to-view-id> 
    </navigation-case>
    

        
</navigation-rule> 
        
</faces-config>
